{
  "name": "typescript-orm-benchmark",
  "version": "0.1.0",
  "description": "Example base for typescript orm benchmark",
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit && eslint \"*/**/*.ts\"",
    "lint-diff": "git diff --name-only --cached --relative | grep \\\\.ts$ | xargs eslint",
    "postinstall": "npm run build",
    "precommit": "npm run lint-diff",
    "knex": "node -r dotenv/config ./node_modules/.bin/knex",
    "migrate:make": "npm run knex -- migrate:make --knexfile ./dist/src/knex/config.js",
    "migrate": "npm run knex -- migrate:latest --knexfile ./dist/src/knex/config.js",
    "start": "ts-node-dev --respawn --transpile-only ./server.ts"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "main": "dist/server.js",
  "author": "Emanuel Casco",
  "license": "MIT",
  "dependencies": {
    "@mikro-orm/core": "^4.5.3",
    "@mikro-orm/postgresql": "^4.5.3",
    "autocannon": "^7.2.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^0.95.4",
    "knex-stringcase": "^1.4.5",
    "objection": "^2.2.15",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.2",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@types/babel-core": "^6.25.6",
    "@types/babel-traverse": "^6.25.5",
    "@types/bluebird": "^3.5.33",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.41",
    "@types/pg": "^7.14.11",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-preset-env": "^1.7.0",
    "dictum.js": "^1.0.4",
    "dotenv": "^8.2.0",
    "eslint": "^7.25.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "sequelize-cli": "^6.2.0",
    "ts-node-dev": "^1.1.6",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  }
}
